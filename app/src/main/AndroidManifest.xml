        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".signin.AccountActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Buyer.BPaymentActivity" />
        <activity android:name=".Buyer.CommondityListActivity" />
        <activity android:name=".Buyer.BMapActivity" />
        <activity
            android:name=".Buyer.HomeBuyerActivity"
            android:label="@string/title_activity_home_buyer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Seller.ProfileActivity" />
        <activity android:name=".Seller.RegistrationActivity" />
        <activity android:name=".Seller.SPaymentActivity"></activity>
    </application>

</manifest>